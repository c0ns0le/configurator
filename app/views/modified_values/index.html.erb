<h1>Listing modified_values</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th>Status</th>
      <th>Organization</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @modified_values.each do |modified_value| %>
      <tr>
        <td><%= modified_value.key %></td>
        <td><%= modified_value.value %></td>
        <td><%= modified_value.status %></td>
        <td><%= modified_value.organization.name %></td>
        <td><%= modified_value.service.name %></td>
        <td><%= link_to 'Show', modified_value %></td>
        <td><%= link_to 'Edit', edit_modified_value_path(modified_value) %></td>
        <td><%= link_to 'Destroy', modified_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Modified value', new_modified_value_path %>
