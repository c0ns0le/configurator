<h1>Release Management</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Config type</th>
      <th>Deleted at</th>
      <th>Expired at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @config_sets.each do |config_set| %>
      <tr>
        <td><%= config_set.name %></td>
        <td><%= config_set.config_type %></td>
        <td><%= config_set.deleted_at %></td>
        <td><%= config_set.expired_at %></td>
        <td><%= link_to 'Show', config_set %></td>
        <td><%= link_to 'Edit', edit_config_set_path(config_set) %></td>
        <td><%= link_to 'Destroy', config_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Config set', new_config_set_path %>



