<h1>ModifiedValues</h1>
<table class="table table-striped">
  <thead>
    <tr>
        <th>Key</th>
        <th>Value</th>
        <th>Status</th>
        <th>Organization</th>
        <th>Service</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="modified_value in modified_values">
      <td>
        <a href="/modified_values/{{modified_value.id}}">
          {{modified_value.key}}
        </a>
      </td>
        <td>
          {{modified_value.value}}
        </td>
        <td>
          {{modified_value.status}}
        </td>
        <td>
          {{modified_value.organization_id}}
        </td>
        <td>
          {{modified_value.service_id}}
        </td>
      <td>
        <a href="/modified_values/{{modified_value.id}}/edit"
          class="btn btn-primary"><i class="icon-edit"></i>
          Edit
        </a>
        <button ng-click="destroy()" class="btn btn-danger">
          <i class="icon-trash"></i> Delete
        </button>
      </td>
    </tr>
  </tbody>
</table>

<p>
  <a href='/modified_values/new' class="btn btn-primary">
    <i class="icon-plus"></i>New
  </a>
</p>
